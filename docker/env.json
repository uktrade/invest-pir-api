{
  "host_env_var_prefix": "PIR_API",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
      "PORT",
      "DATABASE_URL",
      "SECRET_KEY",
      "RECAPTCHA_PUBLIC_KEY",
      "RECAPTCHA_PRIVATE_KEY",
      "NOCAPTCHA",
      "REDIS_URL",
      "CRYPTOGRAPHY_DONT_BUILD_RUST"

    ],
    "optional": [
      "ALLOWED_HOSTS",
      "SECURE_SSL_REDIRECT",
      "AWS_STORAGE_BUCKET_NAME",
      "AWS_ACCESS_KEY_ID",
      "AWS_SECRET_ACCESS_KEY",
      "AWS_S3_CUSTOM_DOMAIN",
      "RESTRICT_ADMIN",
      "ALLOWED_ADMIN_IPS",
      "ALLOWED_ADMIN_IP_RANGES",
      "AUTHBROKER_CLIENT_ID",
      "AUTHBROKER_CLIENT_SECRET",
      "STAFF_SSO_AUTHBROKER_URL",
      "FEATURE_ENFORCE_STAFF_SSO_ENABLED"
    ]
  }
}