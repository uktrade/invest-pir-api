{
  "host_env_var_prefix": "INVEST",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
      "PORT",
      "DATABASE_URL",
      "SECRET_KEY",
      "RECAPTCHA_PUBLIC_KEY",
      "RECAPTCHA_PRIVATE_KEY",
      "NOCAPTCHA",
      "REDIS_URL",
      "ZENDESK_EMAIL",
      "ZENDESK_SUBDOMAIN",
      "ZENDESK_TOKEN"
    ],
    "optional": [
        "ALLOWED_HOSTS",
        "SECURE_SSL_REDIRECT",
        "AWS_STORAGE_BUCKET_NAME",
        "AWS_ACCESS_KEY_ID",
        "AWS_SECRET_ACCESS_KEY",
        "AWS_S3_CUSTOM_DOMAIN"
    ]
  }
}
